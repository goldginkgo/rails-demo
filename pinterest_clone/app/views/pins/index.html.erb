<div class="card-deck">
  <% @pins.each do |pin| %>
  <div class="card">
    <div class="card-block">
      <h4 class="card-title"><%= pin.title %></h4>
    </div>
    <div class="card-footer">
      <small class="text-muted">Submitted by <%= pin.user.email %></small>
    </div>
  </div>
  <% end %>
</div>
